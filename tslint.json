{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-shadowed-variable": false,
    "member-ordering": false,
    "interface-name": false,
    "arrow-parens": false,
    "max-line-length": false,
    "alwaysShowRuleFailuresAsWarnings": true
  },
  "rulesDirectory": []
}
